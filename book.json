{
    "title": "Front End Preprocessing Tool",
    "author": "mk",
    "description" : "Specifications and specifications",
    "language": "zh-hans",
    "plugins": [
        "theme-api",
        "github",
        "anchors",
        "book-summary-scroll-position-saver",
        "expandable-chapters",
        "splitter",
        "anchor-navigation-ex",
        "tbfed-pagefooter",
        "prism",
        "-highlight"
    ],
    "theme-default": {
        "showLevel": true
    },
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/mkzxcvbnm/yunu"
        },
        "tbfed-pagefooter": {
            "copyright":"2017 mk",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        }
    }
}